{
    "name": "freestylewiki-extension",
    "displayName": "FreeStyleWiki for Visual Studio Code",
    "description": "FreeStyleWiki support for Vistual Studio Code",
    "author": {
        "name": "entooone"
    },
    "publisher": "entooone",
    "license": "SEE LICENSE IN LICENSE",
    "version": "0.5.6",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.41.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/entooone/freestylewiki-vscode"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "fsw",
                "aliases": [
                    "FreeStyleWiki",
                    "fsw",
                    "fswiki"
                ],
                "extensions": [
                    ".fsw",
                    ".fswiki"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fsw",
                "scopeName": "source.fsw",
                "path": "./syntaxes/fsw.tmLanguage.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:fsw"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -w -p ./",
        "build": "vsce package"
    },
    "main": "./out/extension.js",
    "devDependencies": {
        "@types/node": "^15.12.2",
        "@types/vscode": "^1.41.0",
        "@typescript-eslint/eslint-plugin": "^4.26.1",
        "@typescript-eslint/parser": "^4.26.1",
        "eslint": "^7.28.0",
        "typescript": "^4.3.2",
        "vsce": "^1.93.0",
        "vscode-test": "^1.5.2"
    }
}
